.main-overlay {
  background: #0a0a0c;
  width: 100%;
  height: 100%;
  position: fixed;
  z-index: 150;
  top: -100%;
  left: 0;
  overflow-y: scroll;
  text-align: center;
  
  -webkit-transform: translateZ(0);
  -moz-transform: translateZ(0);
  -ms-transform: translateZ(0);
  -o-transform: translateZ(0);
  transform: translateZ(0);

  -webkit-transition: background-color 1s ease-out;
  -moz-transition: background-color 1s ease-out;
  -ms-transition: background-color 1s ease-out;
  -o-transition: background-color 1s ease-out;
  transition: background-color 1s ease-out;
  
/*
  padding-top: 64px;

  -webkit-transition: all 1s ease-out .25s;
  -moz-transition: all 1s ease-out .25s;
  -ms-transition: all 1s ease-out .25s;
  -o-transition: all 1s ease-out .25s;
  transition: all 1s ease-out .25s;
*/
}

.main-overlay.loading {
  background-image: url('img/loading.gif');
  background-position: center center;
  background-repeat: no-repeat;
}

.main-overlay .main-overlay-content {

  background: #0a0a0c;
  overflow: auto;

  -webkit-transition: background-color 1s ease-out;
  -moz-transition: background-color 1s ease-out;
  -ms-transition: background-color 1s ease-out;
  -o-transition: background-color 1s ease-out;
  transition: background-color 1s ease-out;
}

.main-overlay .main-overlay-content img.full-width {
  max-width: none;
  width: 100%;
}

.main-overlay h1 {
  font-size: 2em;
  color: #FFF;
}

.main-overlay h5 {
  color: #FFF;
}

.main-overlay hr {
  border-color: #3B3B3D;
}

.main-overlay p {
  color: #FFF;
}

.main-overlay.no-scroll {
  overflow: hidden;
}

/*
.main-overlay.active {
  top: 0%;
}
*/

.main-overlay-menu {
  position: fixed;
  top: 0em;
  right: -12em;
  width: 150px;
  height: 180px;
  overflow: hidden;
  z-index: 200;

  -webkit-transition: right .5s ;
  -moz-transition: right .5s ;
  -ms-transition: right .5s ;
  -o-transition: right .5s ;
  transition: right .5s ;
}

.main-overlay-menu.active {
  right: 0em;
}

.main-overlay-menu .option {
  height: 56px;
  width: 100%;
  cursor: pointer;
  margin-bottom: 1px;
}

.main-overlay-menu .option .name {
  display: none;
  font-family: 'Oswald';
  -webkit-font-smoothing: antialiased;
  font-smoothing: antialiased;
  height: 56px;
  width: 88px;
  background: #383838;
  text-align: center;
  line-height: 56px;
  color: #FFF;
  text-transform: uppercase;
  position: absolute;
  right: -56px;

  -webkit-transition: right .5s ;
  -moz-transition: right .5s ;
  -ms-transition: right .5s ;
  -o-transition: right .5s ;
  transition: right .5s ;
}

.main-overlay-menu .option:hover .name {
  right: 57px;
}

.main-overlay-menu .option .icon {
  height: 56px;
  width: 56px;
  background: #1e1e1e;
  position: absolute;
  right: 0;
  /*top: 0;*/
  z-index: 10;

  -webkit-transition: background .5s ;
  -moz-transition: background .5s ;
  -ms-transition: background .5s ;
  -o-transition: background .5s ;
  transition: background .5s ;
}

.main-overlay-menu .option:hover .icon {
  background-color: #383838;
}

.main-overlay-menu .close .icon {
  background: #1e1e1e url('img/project-button-close.png') center center no-repeat;
}

.main-overlay-menu .prev .icon {
  background: #1e1e1e url('img/project-button-prev.png') center center no-repeat;
}

.main-overlay-menu .next .icon {
  background: #1e1e1e url('img/project-button-next.png') center center no-repeat;
}

.page-slider .slideshow-wrapper {
  width: 100%;
  height: calc(100% - 60px - 60px); /* 60px top, 60px scroll bar at bottom */
  background: #000;
  position: absolute;
  top: 60px;
  left: 0;
  overflow: hidden;
}

.page-slider .slideshow-wrapper ul {
  width: 100%;
  height: 100%;
  position: relative;
  top: 0px;
}

.page-slider .slideshow-wrapper ul li {
  width: 100%;
  height: 100%;
  /*position: absolute;
  left: 0;
  */
  display: block;
  float: left;
}

.page-slider .slideshow-wrapper ul li div {
  width: 100%;
  height: 100%;
}

.page-slider .slideshow-wrapper .slideshow-nav {
  position: absolute;
  bottom: 5%;

  -webkit-transform:scale(.5);
   -moz-transform:scale(.5);
    -ms-transform:scale(.5);
     -o-transform:scale(.5);
        transform:scale(.5);
}

.page-slider .slideshow-wrapper .slideshow-nav .slideshow-button {
  width: 12px;
  height: 12px;
  background: #999;
  float: left;
  margin: 0 10px;
  cursor: pointer;

  -moz-border-radius: 100%;
  -webkit-border-radius: 100%;
  -khtml-border-radius: 100%;
  border-radius: 100%;
}

.page-slider .slideshow-wrapper .slideshow-nav .slideshow-current {
  width: 12px;
  height: 12px;
  background: #2F487B;
  margin: 0 10px;
  pointer-events:none;
  position: relative;

  -moz-border-radius: 100%;
  -webkit-border-radius: 100%;
  -khtml-border-radius: 100%;
  border-radius: 100%;
}

.page-slider .scroll-to-view {
  position: absolute;
  width: 100%;
  height: 60px;
  top: calc(100% - 60px);
  background: #2F487B url('img/scroll-to-view-arrow.png') center center no-repeat;
  cursor: pointer;
}
