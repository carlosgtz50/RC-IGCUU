.mobile-menu {
  z-index: 1000;
  width: 50%;
  height: 100%;
  position: fixed;
  left: 100%;
  top: 0;
  background: #000;
  color: #FFF;
  font-family: 'Oswald';
  -webkit-font-smoothing: antialiased;
  font-smoothing: antialiased;
  font-weight: normal;
  text-indent: 0;
}

.mobile-menu ul {
  list-style: none;
  padding: 5% 0 0 5%;
}

.mobile-menu ul li.menu-item {
  margin-bottom: 0.4em;
}

.mobile-menu ul li.lang-selector {
  float: left;
  margin-top: 1em;
}

.mobile-menu ul li.lang-selector a:after {
  content: "/";
  margin: 0 1em;
}

.mobile-menu ul li.lang-selector:last-of-type a:after {
  content: "";
  margin: 0;
}

.mobile-menu ul li.menu-item a {
  color: #969696;
}

.mobile-menu ul li.menu-item.active a{
  color: #FFF;
}

